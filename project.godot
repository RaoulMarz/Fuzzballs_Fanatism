; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Fuzzballs_Fanatism"
run/main_scene="res://GameIntroScreen.tscn"
config/icon="res://icon.png"

[display]

window/size/width=1600
window/size/height=1200
window/size/resizable=false
window/size/fullscreen=true

[memory]

limits/multithreaded_server/rid_pool_prealloc=120
limits/message_queue/max_size_kb=1512

[network]

limits/debugger_stdout/max_chars_per_second=38000
limits/debugger_stdout/max_messages_per_frame=40
limits/debugger_stdout/max_errors_per_second=150
limits/debugger_stdout/max_warnings_per_second=150

[rendering]

environment/default_environment="res://default_env.tres"
